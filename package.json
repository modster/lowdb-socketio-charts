{
  "name": "lowdb-socketio-charts",
  "version": "0.5.0",
  "description": "lowdb-socketio-charts",
  "author": "emgreeff",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "main": "index.js",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "lowdb",
    "socket.io",
    "lightweight-charts",
    "tradingview",
    "binance"
  ],
  "devDependencies": {
    "jsdoc": "^3.6.10",
    "prettier": "^2.5.1",
    "turbo": "^1.1.2"
  },
  "packageManager": "npm@8.0.0"
}
